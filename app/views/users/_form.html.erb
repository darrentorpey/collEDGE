<% form_for @user do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :key %><br />
    <%= f.text_field :key %>
  </p>
  <p>
    <%= f.label :roles_mask %><br />
    <%= f.text_field :roles_mask %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
